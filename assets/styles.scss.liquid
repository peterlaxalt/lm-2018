// contact.scss

.contact-hidden {
  display:none;
  opacity:0;
  .close-overlay {
    opacity:0;
  }
}

.contact-visible {
  display:block;
  opacity:1;
}

.contact-overlay {
  height:100vh;
  z-index:600;
  position:fixed;
  bottom:0-($footer-height);
  left:$sidenav-size;
  top:0;
  right:0;
  overflow:visible;
  @include fadein(2s);
  .content {
    margin:0;
  }
  .contact-scroll {
    overflow:auto;
    min-height:100vh;
    height:100%;
    padding:$indent;
    padding-bottom:$indent*4;
    @media (max-width:1024px) {
      padding:$indent/2;
    }
    .contact-center {
      @media (min-width:1024px) {
        @include vertical-align;
      }
      @media (min-height:1024px) {
        @include vertical-align;
      }
    }
  }
  .close-overlay {
    opacity:1;
    width:$sidenav-size;
    height:$sidenav-size;
    position:fixed;
    top:$content-spacing;
    left:$content-spacing;
    border: 1px solid $contact-color;
    cursor:pointer;
    .close-x {
      width:$sidenav-size;
      height:$sidenav-size;
      position:relative;
      &:after, &:before {
        content:'';
        position:absolute;
        width:$sidenav-size;
        height:1px;
        background-color:$contact-color;
        top:($sidenav-size/2)-1px; //1px height of lines
      }
      &:before {
        -webkit-transform:rotate(45deg);
        -moz-transform:rotate(45deg);
        transform:rotate(45deg);
        left:-1px;
      }
      &:after {
        -webkit-transform:rotate(-45deg);
        -moz-transform:rotate(-45deg);
        transform:rotate(-45deg);
        left:-1px;
      }
    }
  }
  .page-head {
    margin-top:$indent/2;
    @media (max-width:1024px) {
      font-size:$pagehero-size/2.25;
      line-height:($pagehero-size/2.25)+5px;
      margin: $indent*2 0px $indent 0px;
    }
  }
}

