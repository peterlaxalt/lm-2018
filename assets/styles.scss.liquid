// service styles and the classes in filterKey()

.filter-options {
  line-height:$bodytext-size*1.5;
  font-weight:normal;
  overflow:hidden;
  height:0;
  a, a:active, a:hover, a:visited {
    color:$lm-brown;
  }
  ul {
    list-style-type:none;
    padding-bottom:$indent*2;
  }
  h3 {
    border-bottom:$content-spacing solid $lm-brown;
    display:block;
    margin-bottom:$content-spacing*2;
  }
}

.filter-hide-scroll {
  max-height: 100vh;
  overflow-y: scroll;
}

.filter-open {
  height:auto;
  min-height:100vh;
  overflow:auto;
}

.flip {
  transform: rotate(-90deg) !important;
}

.fade-head {
  opacity:.2 !important;
}

//lmnts.scss


.lmnts-wrapper {
  display: -webkit-box;
  display: -moz-box;
  display: -ms-flexbox;
  display: -webkit-flex;
  display: flex;
  flex-direction: row;
  justify-content: center;
  align-items: flex-start;
  flex-wrap:wrap;
  box-sizing:border-box;
  .content {
    margin:0;
  }
  div[class^="col-"], div[class*=" col-"] {
    float:none;
  }
  .article-box {
    @media (max-width:1024px) {
      padding:$indent $indent/2;
    }

  }
}

// page.scss
body {
  background-color:$wrapper-bg;
  font-family:$bodyfont;
  color:$lm-brown;
  margin:0;
  padding:0;
  -webkit-overflow-scrolling: touch;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
  text-rendering: optimizelegibility;
  @media (max-width:1024px) {
    font-size:$bodytext-size-mobile;
  }
}

::-webkit-scrollbar {
    width: 0px;  /* remove scrollbar space */
    background: transparent;  /* optional: just make scrollbar invisible */
}
/* optional: show position indicator in $lm-brown */
::-webkit-scrollbar-thumb {
    background: $lm-brown;
}

.wrapper {
  z-index:1;
  background-color:$wrapper-bg;
  padding:($content-spacing)/2;
  margin-left:$sidenav-size;
  margin-bottom:$footer-height;
  min-height:100vh;
  @media (max-width:1024px) {
    margin-bottom:$footer-height-mobile;
  }
}

.logotype {
  display:inline-block;
  position:absolute;
  top:$content-spacing;
  right:$content-spacing;
  position:absolute;
  color:$lm-brown;
  @extend .intro-head;
  @media (max-width:1024px) {
    font-size:(($smallhead-size)-2px);
    letter-spacing:$smallhead-spacing;
  }
}

.noscroll {
  overflow:hidden;
}

