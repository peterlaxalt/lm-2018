      &:nth-child(2) {
        @include fadein(4s);
      }
      &:nth-child(3) {
        @include fadein(6s);
      }
      &:nth-child(4) {
        @include fadein(8s);
      }
      &:nth-child(5) {
        @include fadein(10s);
      }
      &:nth-child(1)::before {
        content:'01.';
        @extend .nav-numbers;
      }
      &:nth-child(2)::before {
        content:'02.';
        @extend .nav-numbers;
      }
      &:nth-child(3)::before {
        content:'03.';
        @extend .nav-numbers;
      }
      &:nth-child(4)::before {
        content:'04.';
        @extend .nav-numbers;
      }
      &:nth-child(5)::before {
        content:'05.';
        @extend .nav-numbers;
      }
