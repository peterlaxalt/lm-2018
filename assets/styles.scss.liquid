.side-nav {
  z-index:550;
  width:200vh;
  height:$sidenav-size;
  text-transform:lowercase;
  position:fixed;
  background:$white;
  color:$lm-brown;
  -webkit-transform-origin: left top;
  -webkit-transform:rotate(90deg) translateY(-100%);
  transform-origin: left top;
  transform:rotate(90deg) translateY(-100%);
  .branding {
    float:left;
    position:absolute;
    bottom:$content-spacing*2;
    left:$content-spacing*2;
  }
  ul {
    margin:0;
    list-style-type:none;
    position:absolute;
    bottom:0;
    width:200vh;
    left:$content-spacing*11.25;
    li {
      margin-right:$navspacing;
      float:left;
      height:100%;
