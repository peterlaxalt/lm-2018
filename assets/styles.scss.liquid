// nav
$sidenav-size: 60px;
$navspacing:20px;

// header
$header-height:660px;
// footer
$footer-height:70vh;
$footer-height-mobile:100vh;

// spacing
$content-spacing:7px;
$indent:50px;

// columns
$col-full:100%;
$col-half:($col-full)/2;
$col-third:($col-full)/3;
$col-quarter:($col-half)/2;
$col-eighth:($col-quarter)/2;

// overrides

.padding-indent {
  padding:$indent;
}
.padding-indent-2x {
  padding:$indent*2;
}
.padding-indent-3x {
  padding:$indent*3;
}
.padding-indent-4x {
  padding:$indent*4;
}

.padding-bottom-indent {
  padding-bottom:$indent;
}

.padding-bottom-indent-half {
  padding-bottom:$indent/2;
}

.padding-bottom-indent-2x {
  padding-bottom:$indent*2;
}

.padding-bottom-indent-3x {
  padding-bottom:$indent*3;
}

.padding-top-indent {
  padding-top:$indent;
}

.padding-top-indent-2x {
  padding-top:$indent*2;
}

.padding-left-indent {
  padding-left:$indent;
}

.padding-left-indent-2x {
  padding-left:$indent*2;
}

.padding-right-indent {
  padding-right:$indent;
}

.padding-right-indent-2x {
  padding-right:$indent*2;
}

.padding-reset-sm {
  @media (max-width:1024px) {
    padding:$content-spacing;
  }
}

.padding-clear-sm {
  @media (max-width:1024px) {
    padding:0;
  }
}

.padding-bottom-indent-quarter-sm {
  @media (max-width:1024px) {
    padding-bottom:$indent*.25;
  }
}

.padding-right-indent-half-sm {
  @media (max-width:1024px) {
    padding-right:$indent/2;
  }
}

.padding-left-indent-half-sm {
  @media (max-width:1024px) {
    padding-left:$indent/2;
  }
}

.padding-bottom-indent-sm {
  @media (max-width:1024px) {
    padding-bottom:$indent;
  }
}

.padding-bottom-indent-2x-sm {
  @media (max-width:1024px) {
    padding-bottom:$indent*2;
  }
}

.padding-top-indent-2x-sm {
  @media (max-width:1024px) {
    padding-top:$indent*2;
  }
}

.padding-top-indent-sm {
  @media (max-width:1024px) {
    padding-top:$indent;
  }
}

.window-height {
  height:84vh;
  @media (max-width:1024px) {
    height:600px;
  }
  @media (max-width:676px) {
    height:280px;
  }
}

.center-y {
  @include vertical-align;
}

.center-y-absolute {
  @include vertical-align(absolute);
}

.center-y-reset-sm {
  @media (max-width:1024px) {
    position:relative;
    top:auto;
    -webkit-transform: none;
    -ms-transform: none;
    transform: none;
  }
}

.center-y-fixed {
  @include vertical-align(fixed);
}

.fullheight-sm {
  @media (max-width:1024px) {
    min-height:80vh !important;
  }
}

// palette.scss

$lm-brown:#302A24;
$lm-ice:#F6F8FB;
$lm-yellow:#FCFF5F;
$white:#fff;
$black:#302A24;
$lm-peach:#FF8E6F;

// tertiary

$lm-pink:#FFA8CE;
$lm-lemon:#FCFFE0;
$lm-sky:#6FF9FF;

// styles

$wrapper-bg:$white;
$contact-bg:$lm-yellow;
$contact-color:$lm-brown;


.overlay-color {
  background: -webkit-linear-gradient(to right, $lm-peach, $lm-yellow);
  background: -o-linear-gradient(to right, $lm-peach, $lm-yellow);
  background: -moz-linear-gradient(to right, $lm-peach, $lm-yellow);
  background: linear-gradient(to right, $lm-peach, $lm-yellow);
  background-size: 100% 300%;
  @include tiltgradient(.2s);
  overflow: hidden;
}

@mixin gradient-bg($c1,$c2,$deg,$duration:0s) {
  background: -webkit-linear-gradient($deg, $c1, $c2);
  background: -o-linear-gradient($deg, $c1, $c2);
  background: -moz-linear-gradient($deg, $c1, $c2);
  background: linear-gradient($deg, $c1, $c2);
  background-size: 100% 300%;
  @include tiltgradient($duration);
  overflow: hidden;
}

.bg-img-hero-title {
  background: none;
  background: -webkit-linear-gradient(rgba(0,0,0,0), $black);
  background: -o-linear-gradient(rgba(0,0,0,0), $black);
  background: -moz-linear-gradient(rgba(0,0,0,0), $black);
  background: linear-gradient(rgba(0,0,0,0), $black);
}

// typography

@font-face {
    font-family: 'Otama';
    src: url({{ 'otama.woff' | asset_url }}) format('woff');
    font-weight: normal;
    font-style: normal;
}


// body
$bodyfont: 'Heebo', 'Helvetica', 'Tahoma', sans-serif;
$headerfont: 'Otama', 'Abril Fatface', 'Didot', serif;
$quotefont: 'Lato', sans-serif;

// selection
::-moz-selection {
  background: $lm-pink;
  opacity:100%;
  color:$black;
 }
::selection {
  background: $lm-pink;
  opacity:100%;
  color:$black;
 }



// nav
$navfont: 'Lora', 'Georgia', serif;
$navfont-size: 17px;

// hero
$pagehero-size: 80px;

// header
$label-head-size: 40px;
$article-head-size: 55px;

// paragraph
$smallhead-size: 14px;
$smallhead-spacing: 1.8px;
$smallhead-weight: 300;
$bodytext-size: 25px;
$link-list-size: 25px;
$bodytext-size-mobile: 20px;

// buttons
$button-font-size:$bodytext-size/1.25;

// spacing
@mixin auto-line-height($size, $height:.08) {
  font-size:$size;
  line-height:$size+($size*$height);
}

// hovers
.highlight-hover {
  background-image: -webkit-linear-gradient(left, rgba($white,0) 50%, $lm-yellow 50%);
  background-image: linear-gradient(to right, rgba($white,0) 50%, $lm-yellow 50%);
  background-position: 0;
  background-size: 200.7% 30px;
  background-blend-mode:multiply;
  -webkit-transition: all 0.4s;
  transition: all 0.4s;
  padding-right:$content-spacing;
  &:hover {
    background-position: -99.3%;
  }
}

.highlight-hover-black {
  background-image: -webkit-linear-gradient(left, rgba($white,0) 50%, rgba($lm-brown,1) 50%);
  background-image: linear-gradient(to right, rgba($white,0) 50%, rgba($lm-brown,1) 50%);
  background-position: 0;
  background-blend-mode:multiply;
  background-size: 200.7% 30px;
  -webkit-transition: all 0.4s;
  transition: all 0.4s;
  padding-right:$content-spacing;
  &:hover {
    background-position: -99.3%;
  }
}

.highlight-hover-white {
  background-image: -webkit-linear-gradient(left, rgba($white,0) 50%, rgba($white,1) 50%);
  background-image: linear-gradient(to right, rgba($white,0) 50%, rgba($white,1) 50%);
  background-position: 0;
  background-size: 200.7% 30px;
  -webkit-transition: all 0.4s;
  transition: all 0.4s;
  padding-right:$content-spacing;
  &:hover {
    background-position: -99.3%;
  }
}

.highlight-hover-peach {
  background-image: -webkit-linear-gradient(left, rgba($white,0) 50%, rgba($lm-peach,1) 50%);
  background-image: linear-gradient(to right, rgba($white,0) 50%, rgba($lm-peach,1) 50%);
  background-position: 0;
  background-size: 200.7% 30px;
  background-blend-mode:multiply;
  -webkit-transition: all 0.4s;
  transition: all 0.4s;
  padding-right:$content-spacing;
  &:hover {
    background-position: -99.3%;
  }
}

// headers

.intro-head {
  display:block;
  text-transform:uppercase;
  font-size:$smallhead-size;
  letter-spacing:$smallhead-spacing;
  font-weight:$smallhead-weight;
  color:$lm-brown;
}

.red-head {
  color:$lm-peach !important;
}

.intro-desc {
  display:block;
  font-size:$bodytext-size;
  color:$lm-brown;
}

.label-head {
  font-size:$label-head-size;
  line-height:($label-head-size)+3px;
  font-family:$bodyfont;
  font-weight:700;
  color:$lm-brown;
  margin:0;
  @media (max-width:1024px) {
      @include auto-line-height($label-head-size/1.75);
  }
}

.article-head {
  @include auto-line-height($article-head-size);
  font-family:$bodyfont;
  font-weight:700;
  color:$lm-brown;
  margin:0;
  @media (max-width:1024px) {
      @include auto-line-height($article-head-size/2);
  }
}

.alt-head {
  color:$lm-brown;
  font-family:$headerfont;
  font-weight:normal;
  @include auto-line-height($article-head-size+20px);
}

.page-head {
  font-family:$headerfont;
  font-size:$pagehero-size;
  line-height:$pagehero-size+2px;
  font-weight:normal;
  color:$lm-brown;
  @media (max-width:1024px) {
    font-size:$pagehero-size/2.25;
    line-height:($pagehero-size/2.25)+5px;
  }
}

.block-head {
  font-family:$headerfont;
  font-size:$pagehero-size;
  line-height:$pagehero-size+2px;
  color:$white;
  @media (max-width:1024px) {
    font-size:$pagehero-size/1.75;
    line-height:($pagehero-size/1.75)+5px;
  }
}

.white-head {
  color:$white;
}

// body

p {
  font-size:$bodytext-size;
  line-height:$bodytext-size*2;
  @media (max-width:1024px) {
    @include auto-line-height($bodytext-size-mobile, .6);
  }
}

.quote {
  font-family:$quotefont;
  font-style:italic;
  font-weight:100;
  @include auto-line-height($article-head-size);
  @media (max-width:1024px) {
    @include auto-line-height($article-head-size/2, .6);
  }
}

.quote-alt {
  font-family:$navfont;
  font-style:italic;
  font-weight:100;
  @include auto-line-height($article-head-size);
}

.service-quote {
